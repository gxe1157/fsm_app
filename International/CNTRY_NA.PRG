****  Evelio  7/20/2004

SET TALK OFF
SET COLOR TO W/B
CLEAR
HOLDREC=0

** SET PATH TO C:\JOBS\MYPROG\INTERN~1\

USE C:\JOBS\NYU_REPO\116724\FRN.DBF IN A

USE C:\JOBS\MYPROG\INTERN~1\CTRY_DAT.DBF IN B
SELECT B

INDEX ON C_CODE TO C:\JOBS\MYPROG\INTERN~1\TEMP

SELECT A
GO TOP

DO WHILE .T.
   M.COUNTRY_CO=SPACE(2)
   M.COUNTRY_CO=SUBSTR(COUNTRY_CO,1,2)
   M.COUNTRY=''
   
   HOLDREC=RECNO()
   SELECT B
   SEEK M.COUNTRY_CO
   
   IF FOUND()
      M.COUNTRY=COUNTRY
      @ 10,30 SAY M.COUNTRY_CO+'  '+M.COUNTRY
   ELSE
      M.COUNTRY="** NG **"
   ENDIF

   SELECT A
   GOTO HOLDREC
   REPLACE COUNTRY WITH M.COUNTRY
   SKIP

IF EOF()
   EXIT
ENDIF
   
ENDDO

SELECT  A
BROWSE

WAIT 'PROCESS COMPLETE'
                 
SORT ON COUNTRY,COUNTRY_CO TO FRN_DONE

USE
USE C:\JOBS\NYU_REPO\116724\FRN.DBF
BROWSE FIELD CITY,STATE,COUNTRY,COUNTRY_CO


