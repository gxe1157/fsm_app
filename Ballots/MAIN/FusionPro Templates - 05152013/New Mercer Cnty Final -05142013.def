   <Rule ReturnType="Callback">
      <Name>OnRecordStart</Name>
      <Comment>Called at the beginning of each record in a composition, before other rules.</Comment>
      <Syntax><![CDATA[&cr;&lf;function check_bk(){&cr;&lf;switch (Field(&quot;T_BK_SPLIT1&quot;).toLowerCase())&cr;&lf;{&cr;&lf;&tab;case &quot;2&quot;.toLowerCase():&cr;&lf;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;sPLIT1=&quot;A&quot;;&cr;&lf;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;sPLIT2=&quot;L&quot;;&cr;&lf;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;sPLIT3=&quot;&quot;;&cr;&lf;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;sPLIT4=&quot;&quot;;&cr;&lf;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;break;&cr;&lf;&cr;&lf;&tab;case &quot;3&quot;.toLowerCase():&cr;&lf;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;sPLIT1=&quot;A&quot;;&cr;&lf;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;sPLIT2=&quot;H&quot;;&cr;&lf;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;sPLIT3=&quot;P&quot;;&cr;&lf;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;sPLIT4=&quot;&quot;;&cr;&lf;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;break;&cr;&lf;&cr;&lf;&tab;case &quot;4&quot;.toLowerCase():&cr;&lf;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;sPLIT1=&quot;A&quot;;&cr;&lf;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;sPLIT2=&quot;F&quot;;&cr;&lf;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;sPLIT3=&quot;L&quot;;&cr;&lf;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;sPLIT4=&quot;S&quot;;&cr;&lf;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;break;&cr;&lf;&cr;&lf;&tab;default:&cr;&lf;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;sPLIT1=&quot;A&quot;;&cr;&lf;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;sPLIT2=&quot;&quot;;&cr;&lf;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;sPLIT3=&quot;&quot;;&cr;&lf;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;sPLIT4=&quot;&quot;;&cr;&lf;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;&#32;break;&cr;&lf;}&cr;&lf;&cr;&lf;}&cr;&lf;&cr;&lf;&cr;&lf;function show_pages(){&cr;&lf;&#32;&#32;FusionPro.Composition.SetBodyPageUsage(&quot;my_poll1&quot;,true);&cr;&lf;&#32;&#32;FusionPro.Composition.SetBodyPageUsage(&quot;my_poll2&quot;,true);&cr;&lf;&#32;&#32;FusionPro.Composition.SetBodyPageUsage(&quot;my_poll3&quot;,true);&cr;&lf;&#32;&#32;FusionPro.Composition.SetBodyPageUsage(&quot;my_poll4&quot;,true);&cr;&lf;&#32;&#32;FusionPro.Composition.SetBodyPageUsage(&quot;slip_sheet&quot;,true); &cr;&lf;}&cr;&lf;&cr;&lf;&cr;&lf;if( tab_alpha==&quot;1&quot;){ show_pages() }&cr;&lf;&cr;&lf;check_bk();&cr;&lf;&cr;&lf;if( Field(&quot;TAB_SEP5&quot;) !=tab_alpha ){&cr;&lf;&#32;&#32;if( Field(&quot;TAB_SEP5&quot;)==sPLIT1 || Field(&quot;TAB_SEP5&quot;)==sPLIT2 || Field(&quot;TAB_SEP5&quot;)==sPLIT3 || Field(&quot;TAB_SEP5&quot;)==sPLIT4 &#32;){&cr;&lf;&#32;&#32;&#32;&#32;&#32;show_pages()&cr;&lf;&#32;&#32;}else{FusionPro.Composition.SetBodyPageUsage(&quot;slip_sheet&quot;,true);}&cr;&lf;&#32;&#32;tab_alpha=Field(&quot;TAB_SEP5&quot;);&cr;&lf;&cr;&lf;}else{ FusionPro.Composition.SetBodyPageUsage(&quot;slip_sheet&quot;,false);}&cr;&lf;&cr;&lf;&cr;&lf;]]></Syntax>
   </Rule>
